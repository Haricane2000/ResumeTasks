<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Client.Styles">

    <SolidColorBrush x:Key="DarkBaseBrush" Color="#4212AF" />
    <Color x:Key="DarkBaseColor">#4212AF</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#462B83" />
    <Color x:Key="MediumBaseColor">#462B83</Color>

    <SolidColorBrush x:Key="LightBaseBrush" Color="#7247D7" />
    <Color x:Key="LightBaseColor">#7247D7</Color>

    <SolidColorBrush x:Key="UltraLightBaseBrush" Color="#466FD5" />
    <Color x:Key="UltraLightBaseColor">#466FD5</Color>


    <SolidColorBrush x:Key="DarkBrush" Color="#FFD6D6D6" />
    <Color x:Key="DarkColor">#FFD6D6D6</Color>

    <SolidColorBrush x:Key="MediumBrush" Color="#FFF1F1F1" />
    <Color x:Key="MediumColor">#FFF1F1F1</Color>

    <SolidColorBrush x:Key="LightBrush" Color="white" />
    <Color x:Key="LightColor">white</Color>


</ResourceDictionary>